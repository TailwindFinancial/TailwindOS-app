{
  "expo": {
    "name": "Tailwind",
    "slug": "tailwindos-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/app-icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/transparant-bg-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tailwind.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/app-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.tailwind.app"
    },
    "web": {
      "favicon": "./assets/icons/app-icon.png"
    }
  }
}

